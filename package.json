{
  "name": "country-roads",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "Victor Saa <vis97c@hotmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.19.0",
    "yarn": ">=1.22.4"
  },
  "scripts": {
    "lint:eslint": "eslint --ext .ts,.js --ignore-path .gitignore .",
    "lint:prettier": "prettier . --check --ignore-unknown",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "build": "tsc",
    "start": "node dist/index.js",
    "start:dev": "cross-env DEBUG=1 ts-node-dev --respawn -- src/index.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-status-monitor": "^1.3.4",
    "lodash": "^4.17.21",
    "memory-cache": "^0.2.0"
  },
  "devDependencies": {
    "@open-xamu-co/eslint-config": "^1.0.1",
    "@types/express": "^4.17.17",
    "@types/express-status-monitor": "^1.3.0",
    "@types/lodash": "^4.14.196",
    "@types/memory-cache": "^0.2.3",
    "@types/node": "^20.4.7",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-svelte": "^2.32.4",
    "eslint-plugin-vue": "^9.15.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^3.0.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.1.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn check --integrity && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}